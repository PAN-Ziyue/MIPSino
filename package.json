{
	"name": "mipsino",
	"displayName": "MIPSino",
	"description": "A built-in MIPS assmbler for VScode.",
	"version": "0.0.1",
	"publisher": "ZiYue",
	"homepage": "https://github.com/PAN-Ziyue/MIPSino",
	"repository": {
		"type": "git",
		"url": "https://github.com/PAN-Ziyue/MIPSino"
	},
	"bugs": {
		"url": "https://github.com/PAN-Ziyue/MIPSino/issues",
		"email": "ziyuepan99@outlook.com"
	},
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:mipsino.ToHex"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mipsino.ToHex",
				"title": "MIPSino: Assemble"
			}
		],
		"languages": [
			{
				"id": "mips",
				"aliases": [
					"MIPS Assembly",
					"mips"
				],
				"extensions": [
					".s",
					".mips",
					".spim",
					".asm"
				],
				"configuration": "./src/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "mips",
				"scopeName": "source.mips",
				"path": "./src/mips.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "mips",
				"path": "./src/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}